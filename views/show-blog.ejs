    <link rel="stylesheet" href="/css/blog-style.css">

    <link rel="stylesheet" href="/path/to/styles/default.css">
    

</head>

<body>

    <div class="ui raised very padded text container segment" id="backColor">
        <h2 class="ui header">
            <%=blog.title%>
        </h2>
        <div class="extra">
            <div class="ui label">
                <%=blog.createdAtDate%>
            </div>
            <div class="ui label">
                <%=blog.createdAtTime%>
            </div>
        </div>
        <div class="ui segment" id="backColor2">
            <div class="ui centered medium image" id="displayToBlock">
                <img src="<%=blog.image%>">
            </div>
        </div>
        <div class="description">
            <%-blog.description%>
        </div>
        <hr style="margin-bottom: 1.75rem;">
        <footer style="text-align: -webkit-center;">
            <img style="display: inline;" class="ui avatar image" src="/images/pp.jpg">
            <span>This Blog is written by <i>Gagandeep Singh</i>.</span>
        </footer>
        <hr style="margin-top: 1.75rem;">
    </div>

    <script src="/path/to/highlight.min.js"></script>
    <script>hljs.highlightAll();</script>

    <!-- Semantic UI JS -->
    <script src="//cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.2.10/semantic.min.js"></script>
    <script src="//code.jquery.com/jquery-1.11.1.min.js"></script>

