    <title>Gagan Personal Blog</title>
    <!-- Title Icon -->
    <link rel="icon" href="https://img2.pngio.com/sardar-logo-png-transparent-png-kindpng-sardar-png-860_600.png"
        type="image/x-icon">

    <!-- BootStrap CSS -->
